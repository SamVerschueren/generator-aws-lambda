{
  "name": "<%= functionName %>",
  "version": "0.0.1",
  "description": "<%= functionDescription %>",
  "license": "MIT",
  "author": {
    "name": "<%= name %>",
    "email": "<%= email %>"
  },
  "scripts": {
    "test": "xo && ava",
    "zip": "gulp build"
  },
  "keywords": [
<% keywords.forEach(function(keyword, i) { -%>
    "<%= keyword %>"<%= i < keywords.length-1 ? ',' : '' %>
<% }); -%>
  ],
  "dependencies": {
<% Object.keys(dependencies).forEach(function(dep, i) { -%>
    "<%= dep %>": "<%= dependencies[dep] %>"<%= i < Object.keys(dependencies).length-1 ? ',' : '' %>
<% }); -%>
  },
  "devDependencies": {
    "aws-lambda-pify": "^0.1.0",
    "aws-sdk": "^2.2.8",
    "gulp": "^3.9.0",
    "gulp-install": "^0.6.0",
    "gulp-zip": "^3.0.2",
    "del": "^2.0.2",
    "ava": "*",
    "xo": "*"
  },
  "xo": {
    "ignores": [
      "test/test.js"
    ]
  }
}
