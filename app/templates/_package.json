{
  "name": "<%= functionName %>",
  "version": "0.0.0",
  "description": "<%= functionDescription %>",
  "author": {
    "name": "<%= name %>",
    "email": "<%= email %>"
  },
  "private": true,
  "main": "index.js",
  "scripts": {
    "test": "xo && ava",
    "build": "gulp build"
  },
  "files": [
    "index.js",
    "config.json",
    "lib"
  ],
  "keywords": [
<% keywords.forEach(function(keyword, i) { -%>
    "<%= keyword %>"<%= i < keywords.length-1 ? ',' : '' %>
<% }); -%>
  ],
  "dependencies": {
    "bragg": "^1.0.0",
    "bragg-router": "^1.0.1",
    "bragg-env": "^1.0.1"<%= Object.keys(dependencies).length > 0 ? ',' : '' %>
<% Object.keys(dependencies).forEach(function(dep, i) { -%>
    "<%= dep %>": "<%= dependencies[dep] %>"<%= i < Object.keys(dependencies).length-1 ? ',' : '' %>
<% }); -%>
  },
  "devDependencies": {
    "ava": "*",
    "aws-lambda-pify": "^3.0.0",
    "aws-sdk": "^2.2.33",
    "del": "^2.0.2",
    "dependency-tree": "^4.0.0",
    "gulp": "^3.9.0",<% if (generateDocs) { %>
    "gulp-api-doc": "^1.1.0",<% } %>
    "gulp-install": "^0.6.0",
    "gulp-remove-empty-lines": "0.0.2",
    "gulp-strip-comments": "^1.0.1",
    "gulp-zip": "^3.0.2",
    "xo": "*"
  },
  "xo": {
    "esnext": true
  }
}
